{
  "name": "stylelint-config-recommended-scss",
  "version": "13.0.0",
  "description": "The recommended shareable SCSS config for Stylelint",
  "keywords": [
    "stylelint",
    "stylelint-config",
    "recommended",
    "scss"
  ],
  "repository": "stylelint-scss/stylelint-config-recommended-scss",
  "license": "MIT",
  "author": "kristerkari",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "format": "prettier . --write --ignore-path=.prettierignore",
    "lint": "npm-run-all --parallel lint:*",
    "lint:formatting": "prettier . --check",
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint:md": "remark . --quiet --frail --ignore-path .gitignore",
    "release": "np",
    "test": "jest",
    "watch": "jest --watch"
  },
  "dependencies": {
    "postcss-scss": "^4.0.7",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-scss": "^5.1.0"
  },
  "devDependencies": {
    "@stylelint/prettier-config": "^3.0.0",
    "@stylelint/remark-preset": "^4.0.0",
    "eslint": "^8.48.0",
    "eslint-config-stylelint": "^19.1.0",
    "eslint-plugin-jest": "^27.2.3",
    "jest": "^29.6.4",
    "np": "^8.0.4",
    "npm-run-all2": "^5.0.2",
    "prettier": "^3.0.0",
    "remark-cli": "^11.0.0",
    "stylelint": "^15.10.3"
  },
  "peerDependencies": {
    "postcss": "^8.3.3",
    "stylelint": "^15.10.0"
  },
  "peerDependenciesMeta": {
    "postcss": {
      "optional": true
    }
  }
}
